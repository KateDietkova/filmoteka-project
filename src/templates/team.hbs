<div class='team__container'>
   <button type=' button' class='team__close-btn'></button>
<img class="team__logo-goit" src="https://m.goit.global/ua/new/assets/images/icons/logo.png" alt="ГоАйти лого" width="100" height="30">
<div class="team__box">
    <h2  class='team__title'>GEEK TEAM</h2>
<span class="team__line"></span>
</div>
  <div class='team__content'>
    <ul class='team__list'>
      {{#each this}}
        <li class='team__item'>
            <a target="_blank" href="{{github-link}}"><img
              class='team__image'
              src='{{avatars}}'
              alt="{{name}}"
            />
            </a>
            <div class="team__card">
                    <p  class='team__name'>{{name}}</p>
                <p  class='team__position'>{{position}}</p>
            </div>
                
        </li>
      {{/each}}
    </ul>
  </div>
</div>

